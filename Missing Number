class Solution(object):
    def missingNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        sum = 0
        for i in range(len(nums)+1):
            sum += i
        realSum = 0
        for j in range(len(nums)):
            realSum = realSum + nums[j]
        missingNum = sum - realSum
        return missingNum
